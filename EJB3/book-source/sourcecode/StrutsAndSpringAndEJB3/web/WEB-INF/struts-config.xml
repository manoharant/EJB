<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
  <action-mappings>
	 <action path="/helloworld" type="com.foshanshop.struts.HelloWorldAction" scope="request" validate="false">
        <forward name="print" path="/outpage.jsp"/>   
     </action>
  </action-mappings>
  <controller contentType="text/html;charset=UTF-8" processorClass="com.foshanshop.struts.ControllerProcessor"/>
  <message-resources parameter="ApplicationResources"/>	
</struts-config>
